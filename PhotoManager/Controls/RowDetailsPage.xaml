<Page x:Class="PhotoManager.Controls.RowDetailsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PhotoManager.Controls"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="RowDetailsPage" Loaded="Page_Loaded">

    <Grid>
        <Label x:Name="label" Content="Actual row details." HorizontalAlignment="Left" Height="26" Margin="69,90,0,0" VerticalAlignment="Top" Width="129"/>
        <TextBlock x:Name="skutb" HorizontalAlignment="Left" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Margin="69,127,0,0"/>
        <TextBox x:Name="testbox" HorizontalAlignment="Left" Height="23" Margin="31,177,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="156,176,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Page>
